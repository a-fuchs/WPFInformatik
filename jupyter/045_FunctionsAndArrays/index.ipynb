    "print( a4 )\n",
    "print( a4[1] )\n",
    "print( a4[1][2] )\n",
    "\n",
    "delim()\n",
    "a5 = [(1,2,\"ha\"),-1,[3,[\"test\"],\"u\"]]\n",
    "for obj in a5:\n",
    "    print( obj )\n"
