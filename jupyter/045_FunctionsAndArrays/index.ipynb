    "%serialconnect #--port=COM3 # Windows with more than one COM-Port\n",
    "\n",
    "from time import sleep\n",
    "\n",
    "for iNumber in range(10):\n",
    "    print( \"Die Zahl {} ist {}\".format( iNumber, \"gerade\" if iNumber % 2 == 0 else \"ungerade\" ) )\n",
    "    sleep(0.5)\n",
    "\n",
    "print( \"Done.\" )"
