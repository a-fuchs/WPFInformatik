    "import math\n",
    "\n",
    "iN = 20\n",
    "\n",
    "def isPrime( n ):\n",
    "    for iNum in range( 2, int(math.sqrt(n)) + 1 ):\n",
    "        if ( n % iNum == 0 ) : return False\n",
    "\n",
    "    return True\n",
    "\n",
    "\n",
    "aIsPrime = [(x,isPrime(x)) for x in range(2, iN)]\n",
    "\n",
    "# or:\n",
    "# aY = list( map( isPrime, aX ) )\n",
    "\n",
    "print( aIsPrime )"
